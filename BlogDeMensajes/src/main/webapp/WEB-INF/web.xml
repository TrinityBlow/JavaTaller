<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>listeners.ListenerBaseUsuario</listener-class>
  </listener>
  <servlet>
  	<servlet-name>ServletAddMensaje</servlet-name>
  	<display-name>ServletAddMensaje</display-name>
  	<description></description>
  	<servlet-class>servlets.ServletAddMensaje</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletLogin</servlet-name>
  	<display-name>ServletLogin</display-name>
  	<description></description>
  	<servlet-class>servlets.ServletLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletAddMensaje</servlet-name>
  	<url-pattern>/ServletAddMensaje</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletLogin</servlet-name>
  	<url-pattern>/ServletLogin</url-pattern>
  </servlet-mapping>
</web-app>
